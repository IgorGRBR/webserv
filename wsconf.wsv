server (
	listen 8064
	maxRequestSize 1024
	location /awesome (
		root ./exampleSite/
		return 200
		index awesome.html
		dirListing
		allowMethod POST
		allowMethod GET
		maxRequestSize 5
	)

	location /awesome/thingy (
		root ./exampleSite/test
		return 200
		index cool.html
	)
)

server (
	listen 8095
	serverName cool-site.com
	serverName (www.cool-site.com awesome.com)
	location / (
		root ./exampleSite/test
		return 200
		index cool.html
		maxRequestSize 200
	)
)
